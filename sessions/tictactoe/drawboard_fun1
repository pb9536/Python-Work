#!/usr/bin/python

import sys
args=sys.argv[1:]

def DrawBoard():
	print"\n"
	print "_%s_|_%s_|_%s_" %(args[0],args[1],args[2])
	print "_%s_|_%s_|_%s_" %(args[3],args[4],args[5])
	print " %s | %s | %s " %(args[6],args[7],args[8])
	print "\n"

def replace():
	for i in range(len(args)):
		if args[i] == 'B':
			args[i] = " "  


replace()
DrawBoard()

